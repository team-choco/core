{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "preserve",
      "noEmit": true,
      "isolatedModules": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
    }
  }
  